{
  "version": "2.6.6",
  "packageManager": "pnpm@10.17.0",
  "description": "Vue JSX Vapor Compiler",
  "type": "module",
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "vue-jsx",
    "volar",
    "vapor",
    "babel",
    "compiler"
  ],
  "license": "MIT",
  "homepage": "https://github.com/vuejs/vue-jsx-vapor#readme",
  "bugs": {
    "url": "https://github.com/vuejs/vue-jsx-vapor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/vue-jsx-vapor.git"
  },
  "scripts": {
    "dev": "pnpm run --filter=\"./packages/*\" --parallel dev",
    "build": "pnpm run --filter=\"./packages/*\" --parallel build",
    "typecheck": "tsmc --noEmit",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "test": "vitest",
    "release": "bumpp -r --all -x 'pnpm run changelog'",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "docs:dev": "pnpm run -C ./docs dev",
    "docs:preview": "pnpm run -C ./docs preview",
    "docs:build": "pnpm run -C ./docs build"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.1.4",
    "@ts-macro/tsc": "catalog:",
    "@types/node": "^22.18.6",
    "bumpp": "^10.2.3",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.36.0",
    "tsdown": "^0.12.9",
    "typescript": "^5.9.2",
    "unplugin-raw": "^0.5.1",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vue-jsx-vapor": "workspace:*"
  },
  "pnpm": {
    "overrides": {
      "estree-walker": "2.0.2",
      "ts-macro": "catalog:"
    }
  }
}
