{
  "version": "2.6.6",
  "packageManager": "pnpm@10.17.0",
  "description": "Vue JSX Vapor Compiler",
  "type": "module",
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "vue-jsx",
    "volar",
    "vapor",
    "babel",
    "compiler"
  ],
  "license": "MIT",
  "homepage": "https://github.com/vuejs/vue-jsx-vapor#readme",
  "bugs": {
    "url": "https://github.com/vuejs/vue-jsx-vapor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/vue-jsx-vapor.git"
  },
  "scripts": {
    "dev": "pnpm run --filter=\"./packages/*\" --parallel dev",
    "build": "pnpm run --filter=\"./packages/*\" --parallel build",
    "bench": "node benchmark/bench.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "test": "vitest",
    "release": "bumpp -r --all -x 'pnpm run changelog'",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.1.4",
    "@types/node": "^22.18.6",
    "@vue-jsx-vapor/compiler": "^2.6.8",
    "@vue-jsx-vapor/compiler-oxc": "workspace:*",
    "@vue-jsx-vapor/compiler-rs": "workspace:*",
    "bumpp": "^10.2.3",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.36.0",
    "tinybench": "^5.0.1",
    "tsdown": "^0.12.9",
    "typescript": "^5.9.2",
    "unplugin-raw": "^0.5.1",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  }
}
